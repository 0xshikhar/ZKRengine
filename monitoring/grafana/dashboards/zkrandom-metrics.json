{
    "dashboard": {
        "id": null,
        "title": "ZKRandom Engine Metrics",
        "tags": [
            "zkrandom",
            "randomness",
            "zk-proofs"
        ],
        "style": "dark",
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "Request Volume",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(zkrandom_requests_total[5m])",
                        "legendFormat": "Requests/sec"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 10
                                },
                                {
                                    "color": "red",
                                    "value": 50
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "Success Rate",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(zkrandom_requests_successful_total[5m]) / rate(zkrandom_requests_total[5m]) * 100",
                        "legendFormat": "Success %"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "percent",
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 90
                                },
                                {
                                    "color": "green",
                                    "value": 99
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 6,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "Average Proof Generation Time",
                "type": "stat",
                "targets": [
                    {
                        "expr": "avg(zkrandom_proof_generation_duration_seconds)",
                        "legendFormat": "Avg Time"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s",
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 30
                                },
                                {
                                    "color": "red",
                                    "value": 60
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "id": 4,
                "title": "Active Chains",
                "type": "stat",
                "targets": [
                    {
                        "expr": "zkrandom_active_chains_total",
                        "legendFormat": "Active Chains"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "value"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": null
                                },
                                {
                                    "color": "yellow",
                                    "value": 1
                                },
                                {
                                    "color": "green",
                                    "value": 3
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 18,
                    "y": 0
                }
            },
            {
                "id": 5,
                "title": "Request Rate by Chain",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(zkrandom_requests_total[5m])",
                        "legendFormat": "Chain {{chain_id}}"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Requests/sec",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 9,
                    "w": 12,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 6,
                "title": "Proof Generation Duration",
                "type": "graph",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(zkrandom_proof_generation_duration_seconds_bucket[5m]))",
                        "legendFormat": "95th percentile"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(zkrandom_proof_generation_duration_seconds_bucket[5m]))",
                        "legendFormat": "50th percentile"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Duration (seconds)",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 9,
                    "w": 12,
                    "x": 12,
                    "y": 8
                }
            },
            {
                "id": 7,
                "title": "Error Rate by Type",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(zkrandom_errors_total[5m])",
                        "legendFormat": "{{error_type}}"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Errors/sec",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 9,
                    "w": 12,
                    "x": 0,
                    "y": 17
                }
            },
            {
                "id": 8,
                "title": "zkVerify Job Status",
                "type": "piechart",
                "targets": [
                    {
                        "expr": "zkrandom_zkverify_jobs_by_status",
                        "legendFormat": "{{status}}"
                    }
                ],
                "gridPos": {
                    "h": 9,
                    "w": 12,
                    "x": 12,
                    "y": 17
                }
            },
            {
                "id": 9,
                "title": "Database Connection Status",
                "type": "stat",
                "targets": [
                    {
                        "expr": "zkrandom_database_connected",
                        "legendFormat": "MongoDB"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "mappings": [
                            {
                                "options": {
                                    "0": {
                                        "text": "Disconnected"
                                    }
                                },
                                "type": "value"
                            },
                            {
                                "options": {
                                    "1": {
                                        "text": "Connected"
                                    }
                                },
                                "type": "value"
                            }
                        ],
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": 0
                                },
                                {
                                    "color": "green",
                                    "value": 1
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 0,
                    "y": 26
                }
            },
            {
                "id": 10,
                "title": "Redis Connection Status",
                "type": "stat",
                "targets": [
                    {
                        "expr": "zkrandom_redis_connected",
                        "legendFormat": "Redis"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "mappings": [
                            {
                                "options": {
                                    "0": {
                                        "text": "Disconnected"
                                    }
                                },
                                "type": "value"
                            },
                            {
                                "options": {
                                    "1": {
                                        "text": "Connected"
                                    }
                                },
                                "type": "value"
                            }
                        ],
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": 0
                                },
                                {
                                    "color": "green",
                                    "value": 1
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 6,
                    "y": 26
                }
            },
            {
                "id": 11,
                "title": "API Response Time",
                "type": "graph",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(http_request_duration_ms_bucket[5m]))",
                        "legendFormat": "95th percentile"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(http_request_duration_ms_bucket[5m]))",
                        "legendFormat": "50th percentile"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Response Time (ms)",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 26
                }
            },
            {
                "id": 12,
                "title": "Memory Usage",
                "type": "graph",
                "targets": [
                    {
                        "expr": "process_resident_memory_bytes",
                        "legendFormat": "RSS Memory"
                    },
                    {
                        "expr": "process_heap_bytes",
                        "legendFormat": "Heap Memory"
                    }
                ],
                "yAxes": [
                    {
                        "label": "Memory (bytes)",
                        "min": 0
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 34
                }
            },
            {
                "id": 13,
                "title": "Chain Health Status",
                "type": "table",
                "targets": [
                    {
                        "expr": "zkrandom_chain_health_status",
                        "legendFormat": "{{chain_name}}",
                        "format": "table",
                        "instant": true
                    }
                ],
                "transformations": [
                    {
                        "id": "organize",
                        "options": {
                            "excludeByName": {},
                            "indexByName": {},
                            "renameByName": {
                                "chain_id": "Chain ID",
                                "chain_name": "Chain Name",
                                "Value": "Health Status"
                            }
                        }
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 34
                }
            }
        ],
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "timepicker": {},
        "templating": {
            "list": [
                {
                    "name": "chain_id",
                    "type": "query",
                    "query": "label_values(zkrandom_requests_total, chain_id)",
                    "refresh": 1,
                    "includeAll": true,
                    "allValue": ".*"
                }
            ]
        },
        "annotations": {
            "list": [
                {
                    "name": "Deployments",
                    "datasource": "-- Grafana --",
                    "enable": true,
                    "iconColor": "green",
                    "tags": [
                        "deployment"
                    ]
                }
            ]
        },
        "refresh": "30s",
        "schemaVersion": 27,
        "version": 1,
        "links": []
    }
}